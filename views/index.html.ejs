  <h1><%= set('name') %></h1>

  <div class="hummingbird_graph" id="log">

    <h2><span class="value"></span> Pageviews / Second</h2>

    <div class="axis_left"></div>
    <canvas width="760" height="216"></canvas>
    <div class="axis_right"></div>

  </div>

	<div style="clear: both;"></div>

  <div id="magazines"></div>

	<div style="clear: both;"></div>

	<div id="sites"></div>

  <script>
    $(document).ready(function() {
      var hummingSocket = new Hummingbird.WebSocket.Dashboard();
			Hummingbird.getMagazines();
			Hummingbird.getSites();
      hummingSocket.start();
    });
  </script>
